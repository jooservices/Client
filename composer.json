{
    "name": "jooservices/xclient",
    "description": "This is my package xclient",
    "keywords": [
        "JOOservices",
        "laravel",
        "xclient"
    ],
    "homepage": "https://github.com/jooservices/xclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Viet VU",
            "email": "jooservices@gmail.com",
            "role": "Developer"
        }
    ],
    "version": "0.0.1",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/contracts": "^10.0||^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "jooservices/xlogger": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.59",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.2",
        "phpmd/phpmd": "^2.15",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^11.0.1",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "JOOservices\\XClient\\": "src/",
            "JOOservices\\XClient\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JOOservices\\XClient\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-xclient --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JOOservices\\XClient\\XClientServiceProvider"
            ],
            "aliases": {
                "XClient": "JOOservices\\XClient\\Facades\\XClient"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
